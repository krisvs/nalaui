{
    "name": "manjui",
    "version": "1.3.3",
    "description": "Vue UI library & design system based on Nightshade UI (nightshade)",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/lib/index.d.ts",
            "import": "./dist/bundle.js"
        },
        "./utils": {
            "types": "./dist/lib/utils/index.d.ts",
            "import": "./dist/utils.js"
        },
        "./components.css": "./dist/components.css",
        "./stylesheets.css": "./stylesheets/full.css",
        "./stylesheets/*": "./stylesheets/*"
    },
    "files": [
        "dist",
        "stylesheets"
    ],
    "scripts": {
        "dev": "vite --host",
        "build": "NODE_ENV=production vite build",
        "lint": "eslint . --cache --max-warnings=0",
        "version": "npm run build",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "author": "Christelle Van Sebroeck based on work by Boris Okunskiy (@inca)",
    "license": "ISC",
    "peerDependencies": {
        "vue": "^3.5.13",
        "vue-router": "^4.0.0"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@nodescript/eslint-config": "^2.0.3",
        "@types/node": "^22.10.5",
        "@vitejs/plugin-vue": "^5.2.1",
        "eslint": "^9.17.0",
        "typescript": "^5.7.2",
        "vite": "^6.0.7",
        "vite-plugin-dts": "^4.5.4",
        "vue-router": "^4.5.1"
    },
    "dependencies": {
        "mesh-decorators": "^1.3.0",
        "mesh-ioc": "^4.1.0",
        "nanoevent": "^1.0.0",
        "reflect-metadata": "0.2.2"
    }
}
